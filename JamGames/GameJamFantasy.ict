      "code": "var damageHero = function(hero, dmg) {\n    hero.hp -= dmg;\n    ct.sound.spawn(ct.random.dice('Hurt1', 'Hurt2', 'Hurt3'));\n    if (hero.hp <= 0 && !hero.kill) {\n        hero.kill = true;\n        ct.room.startGame = false;\n        ct.types.copy('FadeOut', 0, 0);\n        setTimeout(function() {\n            var reason = ct.types.copy('DeathScreen', ct.room.x + ct.room.width / 2, ct.room.y + ct.room.height / 2);\n            reason.frame = ct.random.dice(0, 1, 2);\n        }, 1000);\n        ct.sound.fade('HackneyLives', 0, 1000, ct.room.music);\n    }\n}"
