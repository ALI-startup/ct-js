      "oncreate": "this.speed = 3;\nthis.staffOffset = 20;\nthis.form = 'Fire';\nthis.staff = 'Fire';\nthis.staffTick = 0;\nthis.overheat = 0;\nthis.hp = 20;\nthis.hpRegen = 60 * 3;\n\nthis.shootTimer = 0;\nthis.shootPeriod = 45;\nthis.projectile = 'Fireball';\nthis.overheatCost = 10;\n\nthis.overheatDropTimer = 0;\n\nthis.transform = true;\nthis.ctype = 'Hero';",
      "onstep": "/** Movement **/\n\nif (ct.keyboard.down['left'] || ct.keyboard.down['A']) {\n    this.x -= this.speed;\n}\nif (ct.keyboard.down['right'] || ct.keyboard.down['D']) {\n    this.x += this.speed;\n}\nif (ct.keyboard.down['up'] || ct.keyboard.down['W']) {\n    this.y -= this.speed;\n}\nif (ct.keyboard.down['down'] || ct.keyboard.down['S']) {\n    this.y += this.speed;\n}\n\nvar dir = ct.u.pdn(this.x, this.y, ct.mouse.x, ct.mouse.y);\n\nif (dir > 90 && dir <= 270) {\n    this.tx = 1;\n} else {\n    this.tx = -1;\n}\n\nif (ct.place.occupied(this, this.x, this.y, 'Solid')) {\n    this.x = this.xprev;\n    this.y = this.yprev;\n}\nif (this.x !== this.xprev || this.y !== this.yprev) {\n    this.imgspd = 0.25;\n} else {\n    this.imgspd = 0;\n    this.frame = 0;\n}\n\nthis.depth = this.y;\n\n/** Shooting **/\n\nthis.shootTimer --;\nif (ct.mouse.down) {\n    if (this.shootTimer <= 0) {\n        this.shootTimer = this.shootPeriod;\n        var projectile = ct.types.copy(\n            this.projectile,\n            this.x + ct.u.ldx(this.staffOffset * 2, dir), this.y - 12 + ct.u.ldy(this.staffOffset * 2, dir)\n        );\n        projectile.dir += dir;\n        var overheatPrev = this.overheat;\n        if (this.overheat > 100) {\n                if (this.hp <= 10) {\n                    this.kill = true;\n                    ct.room.startGame = false;\n                    ct.types.copy('FadeOut', 0, 0);\n                    var me = this;\n                    setTimeout(function() {\n                        var reason = ct.types.copy('DeathScreen', ct.room.x + ct.room.width / 2, ct.room.y + ct.room.height / 2);\n                        if (me.form === 'Fire') {\n                            reason.frame = 3;\n                        } else if (me.form === 'Ice') {\n                            reason.frame = 4;\n                        } else if (me.form === 'Rock') {\n                            reason.frame = 6;\n                        } else {\n                            reason.frame = 5;\n                        }\n                    }, 1000);\n                    ct.sound.fade('HackneyLives', 0, 1000, ct.room.music);\n                } else {\n                    this.hp -= 10;\n                    this.overheat = 0;\n                    ct.sound.spawn('Shotgun');\n                    ct.sound.spawn('Hurt3');\n                }\n        }\n        this.overheat += this.overheatCost;\n        catchOverheatStep(overheatPrev, this.overheat);\n    }\n}\n\n/** Overheat **/\n\nthis.overheatDropTimer --;\nif (this.overheatDropTimer <= 0) {\n    this.overheatDropTimer = 20;\n    this.overheat --;\n    if (this.overheat >= 99 && this.overheat < 100) {\n        ct.sound.spawn('Vent');\n    }\n    if (this.overheat < 0) {\n        this.overheat = 0;\n    }\n}\n\n/** Form **/\n\nthis.graph = this.form + 'Form';\n\n/** Regen **/\nthis.hpRegen --;\nif (this.hpRegen <= 0) {\n    if (this.hp < 20) {\n        this.hp++;\n    }\n    this.hpRegen = 60 * 7;   \n}",
