      "code": "var bossDamageHandler = function (boss) {\n    if (!boss.hp) {\n        boss.hp = 1000;\n        boss.hpPrev = boss.hp;\n    }\n    if (boss.hpPrev !== boss.hp) {\n        ct.room.bossHealth -= boss.hpPrev - boss.hp;\n    }\n    boss.hpPrev = boss.hp;\n    if (ct.room.bossHealth <= 0) {\n        boss.kill = true;\n        ct.room.bossHealth = 0;\n        ct.types.each(function () {\n            if (this.ctype === 'Enemy' || this.ctype === 'EnemyBullet' || this.type === 'Summoner') {\n                this.kill = true;\n            }\n        });\n        ct.types.copy('BossDeath', boss.x, boss.y);\n    }\n};"
