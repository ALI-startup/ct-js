    {
      "name": "enemyBehaviour",
      "code": "var enemyBehaviourOld = function (copy) {\n    if (copy.freezeTimer && copy.freezeTimer > 0) {\n        copy.freezeTimer --;\n        copy.spd = 0;\n        return;\n    }\n    if (copy.poisionTimer && copy.poisionTimer > 0) {\n        copy.poisionTimer --;\n    }\n    if (copy.electrocutedTimer && copy.electrocutedTimer > 0) {\n        copy.electrocutedTimer --;\n    }\n    var hero = ct.types.list['TheWizard'][0];\n    if (!copy.behaviourTimer) {\n        copy.behaviourTimer = Math.floor(ct.random.range(20, 60));\n        if (copy.burning) {\n            copy.hp -= 1;\n            if (copy.hp <= 0) {\n                copy.kill = true;\n                return;\n            }\n        }\n        if (hero) {\n            var dist = ct.u.pdc(copy.x, copy.y, hero.x, hero.y),\n                dir = ct.u.pdn(copy.x, copy.y, hero.x, hero.y) + ct.random.range(-60, 60);\n            if (dist < 80) {\n                copy.xx = hero.x + ct.random.range(-40, 40);\n                copy.yy = hero.y + ct.random.range(-40, 40);\n                if (dist <= 40) {\n                    damageHero(hero, copy.damage);\n                }\n            } else {\n                copy.xx = copy.x + ct.u.ldx(200, dir);\n                copy.yy = copy.x + ct.u.ldy(200, dir);\n                if (dist <= 10) {\n                    copy.spd = 0;\n                } else {\n                    copy.spd = copy.speed;\n                    if (copy.poisionTimer && copy.poisionTimer > 0) {\n                        copy.spd /= 2;\n                    }\n                }\n            }\n            copy.xx = Math.min(Math.max(32, copy.xx), ct.room.width - 32);\n            copy.yy = Math.min(Math.max(32, copy.yy), ct.room.height - 32);\n            copy.dir = ct.u.pdn(copy.x, copy.y, copy.xx, copy.yy);\n        } else {\n            copy.spd = 0;\n        }\n    } else {\n        copy.behaviourTimer --;\n    }\n}\n\nvar enemyBehaviour = function (copy) {\n    copy.spd = 0;\n    var spd = copy.speed;\n    if (!copy.effectsTick) {\n        copy.effectsTick = 1;\n    } else {\n        copy.effectsTick ++;\n    }\n    if (copy.freezeTimer && copy.freezeTimer > 0) {\n        copy.freezeTimer --;\n        spd = 0;\n        return;\n    }\n    if (copy.poisionTimer && copy.poisionTimer > 0) {\n        copy.poisionTimer --;\n        spd /= 2;\n    }\n    var hero = ct.types.list['TheWizard'][0];\n    if (!copy.behaviourTimer) {\n        copy.behaviourTimer = Math.floor(ct.random.range(20, 60));\n        if (copy.burning) {\n            copy.hp -= 1;\n            if (copy.hp <= 0) {\n                copy.kill = true;\n                return;\n            }\n        }\n        if (hero) {\n            var dist = ct.u.pdc(copy.x, copy.y, hero.x, hero.y),\n                dir = ct.u.pdn(copy.x, copy.y, hero.x, hero.y) + ct.random.range(-60, 60);\n            if (dist < 80) {\n                copy.xx = hero.x + ct.random.range(-40, 40);\n                copy.yy = hero.y + ct.random.range(-40, 40);\n                if (dist <= 40) {\n                    damageHero(hero, copy.damage);\n                }\n            } else {\n                copy.xx = copy.x + ct.u.ldx(200, dir);\n                copy.yy = copy.x + ct.u.ldy(200, dir);\n                if (dist <= 10) {\n                    spd = 0;\n                }\n            }\n            copy.dir = ct.u.pdn(copy.x, copy.y, copy.xx, copy.yy);\n        } else {\n            spd = 0;\n        }\n    } else {\n        copy.behaviourTimer --;\n    }\n    if (spd !== 0) {\n        ct.place.go(copy, copy.xx, copy.yy, spd, 'Solid');\n    }\n}"
