    {
      "name": "enemyDraw",
      "code": "var enemyDraw = function (copy) {\n    var cx = Math.floor(copy.x),\n        cy = Math.floor(copy.y);\n    if (copy.dir > 90 && copy.dir <= 270) {\n        copy.tx = 1;\n    } else {\n        copy.tx = -1;\n    }\n    ct.draw.img('Shadow', 0, cx, cy);\n    ct.draw(copy, cx, cy);\n    if (copy.burning) {\n        ct.draw.img('BurnEffect', Math.floor((copy.effectsTick / 5) % 5), cx, cy);\n    }\n    if (copy.electrocutedTimer) {\n        ct.draw.img('Electrocuted', Math.floor((copy.effectsTick / 5) % 8), cx, cy);\n    }\n    if (copy.freezeTimer) {\n        ct.draw.img('IceEffect', 4, cx, cy);\n    }\n}"
